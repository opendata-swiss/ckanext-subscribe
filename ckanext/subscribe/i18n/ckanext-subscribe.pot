#: ckanext/subscribe/notification_email.py:36
#, python-format
msgid ""
"\n"
"<p>Changes have occurred in relation to your subscription(s)</p>\n"
"\n"
"{% for notification in notifications %}\n"
"\n"
"  <h3><a href=\"{{ notification.object_link }}\">\"{{ "
"notification.object_title }}\" ({{ notification.object_name }})</a>:</h3>\n"
"\n"
"  {% for activity in notification.activities %}\n"
"    <p>\n"
"      - {{ activity.timestamp.strftime('%Y-%m-%d %H:%M') }} -\n"
"      {{ activity.activity_type }}\n"
"      {% if notification.object_type != 'dataset' %}\n"
"        - {{ activity.dataset_link }}\n"
"      {% endif %}\n"
"    </p>\n"
"  {% endfor %}\n"
"{% endfor %}\n"
"\n"
"--\n"
"{{ html_footer }}\n"
msgstr ""

#: ckanext/subscribe/notification_email.py:57
#, python-format
msgid ""
"\n"
"Changes have occurred in relation to your subscription(s)\n"
"\n"
"{% for notification in notifications %}\n"
"  \"{{ notification.object_title }}\" - {{ notification.object_link }}\n"
"\n"
"  {% for activity in notification.activities %}\n"
"      - {{ activity.timestamp.strftime('%Y-%m-%d %H:%M') }} - {{ "
"activity.activity_type }} {% if (\n"
"          notification.object_type != 'dataset') %} - {{ "
"activity.dataset_href }} {% endif %}\n"
"\n"
"  {% endfor %}\n"
"{% endfor %}\n"
"\n"
"--\n"
"{{ plain_text_footer }}\n"
msgstr ""

